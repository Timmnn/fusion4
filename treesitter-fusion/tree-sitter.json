{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "fusion",
      "camelcase": "Fusion",
      "title": "Fusion",
      "scope": "source.fusion",
      "file-types": [
        "fusion"
      ],
      "injection-regex": "^fusion$",
      "class-name": "TreeSitterFusion"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Fusion grammar for tree-sitter",
    "authors": [
      {
        "name": "Timm Nicolaizik"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-fusion"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}