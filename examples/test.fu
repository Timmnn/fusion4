_c_import "<stdio.h>"

struct Player = {
	name: string
	health: float
}

fn get_name(player: Player) string {
	return player.name
}

fn get_health(player: Player) float {
	return player.health * 13.785
}


let x = 1.58 as float
let name = "Hans" as string

let player = Player { name: name, health: x } as Player

printf("%s\n", get_name(player))
printf("%f\n", get_health(player))
